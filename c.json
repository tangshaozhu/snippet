{
	// Place your snippets for c here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Function Header": {
		"prefix": "fc",
		"body": [
			"/***************************************************",
			" * function: ",
			" * author: ",
			" * input parameters: ",
			" * return value: ",
			" * description: ",
			" **************************************************/"
		],
		"description": ""
	},
	"File Header": {
		"prefix": "hd",
		"body": [
			"/**",
            "********************************************************************************",
            "  * @file    ${TM_FILENAME}",
            "  * @author  Maxic Firmware Team",
            "  * @version V1.0.0",
            "  * @date    ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
            "  * @brief   ",
            " **************************************************************",
            "  * @attention",
            "********************************************************************************",
            "  **/",
            "",
            "/* Define to prevent recursive inclusion -------------------------------------*/",
            "#ifndef __${TM_FILENAME_BASE/([A-Za-z0-9]+)([^A-Za-z0-9]*)/${1:/upcase}${2:+_}/g}_H__",
            "#define __${TM_FILENAME_BASE/([A-Za-z0-9]+)([^A-Za-z0-9]*)/${1:/upcase}${2:+_}/g}_H__",
            "",
            "#ifdef __cplusplus",
            "extern \"C\" {",
            "#endif",
            "",
            "/* Includes ------------------------------------------------------------------*/",
            "",
            "/* Exported defines ----------------------------------------------------------*/",
            "",
            "/* Exported typedefs ---------------------------------------------------------*/",
            "",
            "/* Exported constants --------------------------------------------------------*/",
            "",
            "/* Exported macros -----------------------------------------------------------*/",
            "",
            "/* Exported variables --------------------------------------------------------*/",
            "",
            "/* Exported functions --------------------------------------------------------*/",
            "",
            "",
            "",
            "#ifdef __cplusplus",
            "}",
            "#endif",
            "",
            "#endif /* __${TM_FILENAME_BASE/([A-Za-z0-9]+)([^A-Za-z0-9]*)/${1:/upcase}${2:+_}/g}_H__ */",
            "",
            "/************************ (C) COPYRIGHT MaxIC *****END OF FILE*****************/",
            "",
		],
		"description": ""
	}
}